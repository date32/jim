ver.1 = リリース {
    ログイン・ログアウト
    admin = [ユーザー登録・一覧・削除、
            マシーン登録・一覧・変更・削除、
            エリア登録・一覧・変更・削除
            ],

    dashboard = [トレーニング内容登録、
                記録参照（全部・今月）、
                マシーン毎のトレーニング軌跡
                ],
}

ver.2 = リリース {
    バグ修正 = [
                各エディット
                名前を変更せずに画像のみの変更は画像の名称が「.jpg」となる
                画像を選ばなければ空になる
                →OK

                データ（マシン、エリア）が無い時に「今までの記録を見る」をクリックするとエラー
                →OK

                トレーニング内容登録
                時間がnullであれば"0"を代入
                速さnullのまま
                距離を小数点許可 integer→decimal
                ],
    UI/UX修正 = [
                dashboard
                プルダウン、画像、タイプのマージンを開ける
                携帯用画像見やすく変更
                →OK

                record
                マシーン別トレーニング軌跡
                画像と画像にマージンを開ける
                →OK
                ],
     背景画像挿入 =  [
                     トップページ、ダッシュボード、トレーニングの記録、マシーン別トレーニング軌跡
                     ],
        
}
